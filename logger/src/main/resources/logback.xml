<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{dd-MM-yyyy HH:mm:ss} %-5p %40.40c - %m%n</pattern>
        </encoder>
    </appender>

    <logger name="com.ortecfinance" level="DEBUG"/>
    <logger name="com.querydsl" level="ERROR"/>

    <root level="WARN">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>